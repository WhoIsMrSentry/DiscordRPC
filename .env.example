# Discord Developer Portal -> Applications -> (Your app)
DISCORD_CLIENT_ID=
# Discord Rich Presence image alanı: asset key veya (sende çalışıyorsa) URL
# Önerilen: Discord Developer Portal'a yüklediğin asset key
DISCORD_LARGE_IMAGE=
# Not: Small image bazı istemcilerde sorun çıkarabiliyor; bu projede kullanılmıyor.
DISCORD_SMALL_IMAGE=
# Optional: Join secret (only if you use it)
DISCORD_JOIN_SECRET=

# Discord RPC bağlantı ayarları (opsiyonel)
# Bazı sistemlerde pipe değiştirerek stabil çalışır. Boş bırakılırsa 0-9 denenir.
DISCORD_RPC_PIPE=
DISCORD_CONNECTION_TIMEOUT=30
DISCORD_RESPONSE_TIMEOUT=30

# Eski isimler (geri uyumluluk)
DISCORD_LARGE_IMAGE_KEY=
DISCORD_SMALL_IMAGE_KEY=

# Spotify Developer Dashboard
SPOTIPY_CLIENT_ID=
SPOTIPY_CLIENT_SECRET=
# Must match Spotify app redirect URI settings
SPOTIPY_REDIRECT_URI=http://localhost:8080

# Spotify verisini çekme sıklığı (saniye). RPC güncellemesinden bağımsızdır.
SPOTIFY_POLL_SECONDS=30

# Poll interval (seconds)
POLL_SECONDS=15

# Riot Developer Portal
RIOT_API_KEY=
